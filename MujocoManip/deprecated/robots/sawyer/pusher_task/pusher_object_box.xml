<!-- All objects for pusher are centered at 0 0 and bottom touches z = 0 -->
<!-- They must have body name "pusher_object" and free joint named "pusher_object_free_joint"-->
<mujoco model="pusher_object_box">
	<worldbody>
	<body name="pusher_object" pos="0 0 0">
		<geom pos="0 0 0.05" size="0.05 0.05 0.05" type="box" rgba="1 0 0 1"/>
		<!-- Visualization of object in group 1 -->
		<geom pos="0 0 0.05" size="0.048 0.048 0.048" type="box" rgba="1 0 0 1" group="1" conaffinity="0" contype="0"/>
		<joint type="free" name="pusher_object_free_joint"/>
	</body>	
	<geom name="pusher_target_g0" pos="0 0 0.05" size="0.049 0.049 0.049" type="box" rgba="0 1 0 1" conaffinity="0" contype="0"/>
	<geom name="pusher_target_g1" pos="0 0 0.05" size="0.047 0.047 0.047" type="box" rgba="0 1 0 1" conaffinity="0" contype="0" group="1"/>
</worldbody>
</mujoco>