<!-- sample of a general object defined in mujoco -->
<mujoco model="banana">
	<asset>
		<mesh file="meshes/banana.stl" name="banana_mesh" scale="0.5 0.5 0.5"/>
		<texture file="../textures/metal.png" type="2d" name="tex-metal"/>
		<material name="metal" reflectance="0.7" texrepeat="1 1" texture="tex-metal" texuniform="true"/>
	</asset>
	<worldbody>
		<body>
			<body name="collision">
				<geom pos="0 0 0" mesh="banana_mesh" type="mesh" solimp="0.998 0.998 0.001" solref="0.001 1" density="500" friction="0.5"  rgba="1 1 0 1" group="1"/>
			</body>
			<body name="visual">
				<geom pos="0 0 0" mesh="banana_mesh" type="mesh" rgba="1 1 0 1"  conaffinity="0" contype="0"  group="1"/>
			</body>
			<site pos="0 0 -0.02" name="bottom_site"/>
			<site pos="0 0 0.02" name="top_site"/>
			<site pos="0.095 0.09 0" name="horizontal_radius_site"/>
		</body>
	</worldbody>
</mujoco>