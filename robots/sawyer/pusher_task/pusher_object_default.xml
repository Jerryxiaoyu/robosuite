<!-- All objects for pusher are centered at 0 0 and bottom touches z = 0 -->
<!-- They must have body name "pusher_object" and free joint named "pusher_object_free_joint"-->
<mujoco model="pusher_object_default">
	<worldbody>
	<body name="pusher_object" pos="0 0 0">
		<geom pos="0 0 0.1" size="0.1" type="sphere" rgba="1 0 0 1"/>
		<geom pos="0 0 0.1" size="0.098" type="sphere" rgba="1 0 0 1" group="1" conaffinity="0" contype="0"/>
		<joint type="free" name="pusher_object_free_joint"/>
	</body>	
	<geom name="pusher_target_g0" pos="0 0 0.1" size="0.099" type="sphere" rgba="0 1 0 1" conaffinity="0" contype="0"/>
	<geom name="pusher_target_g1" pos="0 0 0.1" size="0.097" type="sphere" rgba="0 1 0 1" conaffinity="0" contype="0" group="1"/>
</worldbody>
</mujoco>